<% layout("layouts/boilerplates") %>
<div class = "row">
    <div class = "col-4 offset-4">
        <div class="alert alert-danger" role="alert">
          <h4 class="alert-heading">Error <%=status%></h4>
          <hr>
          <% if (process.env.NODE_ENV !== "production") { %>
            <p class="mb-0"><%=message%></p>
            <% } %>
        </div>
    </div>
</div>